#ifndef BLACKJACK_H_
#define BLACKJACK_H_


#define DECK_SIZE 52
typedef struct card {
	enum  {Heart, Diamond, Spade, Club} face;
	enum   {red, black} color;
	unsigned int value;

} card;
card deck[52];
#endif /* BLACKJACK_H_ */
